<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="tadcka_routing.form_type.route.class">Tadcka\Component\Routing\Form\Type\RouteType</parameter>
    </parameters>

    <services>

        <!--Route form type-->
        <service id="tadcka_routing.form_type.route" class="%tadcka_routing.form_type.route.class%">
            <tag name="form.type" alias="tadcka_route" />
            <argument>%tadcka_routing.model.route.class%</argument>
        </service>

    </services>

</container>
