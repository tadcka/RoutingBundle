{
    "name":         "tadcka/routing-bundle",
    "homepage":     "https://github.com/tadcka/RoutingBundle",
    "description":  "Routing bundle on Symfony2",
    "keywords":     ["routing", "symfony routing", "cmf routing"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors": [
        {
            "name":   "Tadas Gliaubicas",
            "email":  "tadcka89@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle" : "~2.7|~3.0",
        "tadcka/routing": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "autoload": {
        "psr-4": {
            "Tadcka\\Bundle\\RoutingBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
